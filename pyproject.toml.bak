[build-system]
requires = ["setuptools>=61.0", "seleniumbase"]
build-backend = "setuptools.build_meta"

[project]
name = "selenium-script"
version = "0.0.3b"
authors = [
  { name="Fakesum", email="worldanvilbild+github@gmail.com" },
]
description = "A Easy Running and Testing Env for Selenium."
readme = "README.md"
requires-python = ">=3.9"
classifiers = [
    "Programming Language :: Python :: 3",
    "License :: OSI Approved :: MIT License",
    "Operating System :: OS Independent",
]

[project.urls]
"Homepage" = "https://github.com/Fakesum/selenium-script"
"Bug Tracker" = "https://github.com/Fakesum/cimport/selenium-script"